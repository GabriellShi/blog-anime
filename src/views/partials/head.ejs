        "@graph": [
          {
            "@type": "CollectionPage",
            "@id": "https://www.gogeekanimes.com/",
            "url": "https://www.gogeekanimes.com/",
            "name": "Go Geek - Site de notícias de animes, mangas, doramas, games, recomendações e curiosidades. Somos sua principal fonte de notícias do mundo geek.",
