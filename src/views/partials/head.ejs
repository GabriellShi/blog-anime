            "about": { "@id": "https://www.gogeekanimes.com/#organization" },
            "description": "Site de notícias de animes, mangas, doramas, games, recomendações e curiosidades. Somos sua principal fonte de notícias do mundo geek",
            "breadcrumb": { "@id": "https://www.gogeekanimes.com/#breadcrumb" },
            "inLanguage": "pt-BR"
          },
          {
            "@type": "BreadcrumbList",
            "@id": "https://www.gogeekanimes.com/#breadcrumb",
            "itemListElement": [
              { "@type": "ListItem", "position": 1, "name": "Início" }
            ]
          },
          {
            "@type": "WebSite",
            "@id": "https://www.gogeekanimes.com/#website",
            "url": "https://www.gogeekanimes.com/",
            "name": "Go Geek",
            "description": "Site de notícias de animes, mangas, doramas, games, recomendações e curiosidades. Somos sua principal fonte de notícias do mundo geek.",
            "publisher": { "@id": "https://www.gogeekanimes.com/#organization" },
            "potentialAction": [
              {
                "@type": "SearchAction",
                "target": {
                  "@type": "EntryPoint",
                  "urlTemplate": "https://www.gogeekanimes.com/?s={search_term_string}"
                },
                "query-input": "required name=search_term_string"
              }
            ],
            "inLanguage": "pt-BR"
          },
          {
            "@type": "Organization",
            "@id": "https://www.gogeekanimes.com/#organization",
            "name": "Go Geek Animes",
            "url": "https://www.gogeekanimes.com/",
            "logo": {
              "@type": "ImageObject",
              "inLanguage": "pt-BR",
              "@id": "https://www.gogeekanimes.com/#/schema/logo/image/",
              "url": "https://www.gogeekanimes.com/img/logo.png",
              "contentUrl": "https://www.gogeekanimes.com/img/logo.png",
              "width": 270,
